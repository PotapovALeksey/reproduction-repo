{
  "name": "investo",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:real": "adb uninstall \"com.newage.investo\" && npm run android",
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "ios:real": "react-native run-ios --device \"iPhone\"",
    "asset": "npx react-native-asset",
    "link-ios": "pod install --project-directory=ios",
    "link-update-ios": "npm run link-ios --repo-update",
    "clean-android": "cd ./android && ./gradlew clean --stacktrace",
    "clean": "react-native-clean-project",
    "start": "react-native start",
    "start:storybook": "rnstl && cross-env APP_ENV=storybook yarn start",
    "test": "jest",
    "build:android": "cd ./android && ./gradlew app:assembleRelease",
    "release:android": "cd ./android && ./gradlew bundleRelease",
    "lint:scripts": "eslint 'src' --quiet --cache",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "lint:styles": "stylelint 'src'",
    "lint": "npm-run-all --parallel lint:scripts lint:styles",
    "prettier-format": "prettier 'src' --write",
    "prettier-check": "prettier 'src' --check",
    "locales": "phrase pull && node scripts/generateLocalizationEnum.js",
    "imagemin": "node scripts/imagemin.js && node scripts/generateImageData.js",
    "imagemin-svg": "svgo -f src/assets/icons && node scripts/generateIconData.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-content-loader": "6.2.0",
    "react-intl": "6.2.7",
    "react-native": "0.71.1",
    "react-native-config": "1.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-react-jsx": "7.20.13",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.13",
    "@react-native-community/eslint-config": "3.2.0",
    "@rnx-kit/dep-check": "1.14.0",
    "@tsconfig/react-native": "2.0.3",
    "@types/react": "18.0.27",
    "@types/react-native": "0.71.1",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "babel-jest": "29.4.1",
    "babel-loader": "9.1.2",
    "babel-plugin-formatjs": "10.3.36",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "metro-react-native-babel-preset": "0.74.1",
    "react-dom": "18.2.0",
    "react-native-clean-project": "4.0.1",
    "react-native-codegen": "0.72.0",
    "react-native-storybook-loader": "2.0.5",
    "react-native-web": "0.18.12",
    "sanitize-filename": "1.6.3",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.9.4"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx"
    }
  },
  "rnx-kit": {
    "reactNativeVersion": "0.69.1",
    "kitType": "app",
    "capabilities": [
      "animation",
      "babel-preset-react-native",
      "core",
      "core-android",
      "core-ios",
      "gestures",
      "jest",
      "modal",
      "navigation/native",
      "navigation/stack",
      "react",
      "react-dom",
      "safe-area",
      "screens",
      "storage",
      "svg"
    ]
  }
}
